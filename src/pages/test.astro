---
let dukkar = "& Astro";
---

<head>
    <script
        defer
        src="https://cdn.jsdelivr.net/npm/alpinejs@3.x.x/dist/cdn.min.js"
    ></script>
</head>
<body
    x-data=`{
        message: "I Love Alpine ${dukkar}",
        src: " favicon.png",
    }`
>
    <h1 x-text="message">sdf</h1>
    <button type="button" @click="message = 'Hello world'">Click</button>
    <img src="{{src}}" alt="" srcset="" />
</body>
